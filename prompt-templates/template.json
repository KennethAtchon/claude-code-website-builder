        "aesthetic": "{{themeAesthetic}}",
        "mood": "{{themeMood}}",
        "targetAudience": "{{themeTargetAudience}}",
        "designPrinciples": "{{themeDesignPrinciples}}",
        "gradientStyle": "{{themeGradientStyle}}"
      },
      "styles": {
        "colors": {
          "primary": "{{primaryColor}}",
          "secondary": "{{secondaryColor}}",
          "accent": "{{accentColor}}",
          "background": "{{backgroundColor}}",
          "text": "{{textColor}}"
        },
        "typography": {
