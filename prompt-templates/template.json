{
    "website": {
      "name": "{{websiteName}}",
      "theme": {
        "name": "{{themeName}}",
        "aesthetic": "{{themeAesthetic}}",
        "mood": "{{themeMood}}",
        "targetAudience": "{{themeTargetAudience}}",
        "designPrinciples": "{{themeDesignPrinciples}}",
        "gradientStyle": "{{themeGradientStyle}}"
      },
      "styles": {
        "colors": [
          {
            "name": "Primary",
            "hex": "{{primaryColor}}"
          },
          {
            "name": "Primary Dark",
            "hex": "{{primaryDarkColor}}"
          },
          {
            "name": "Light 1",
            "hex": "{{lightColor1}}"
          },
          {
            "name": "Light 2",
            "hex": "{{lightColor2}}"
          },
          {
            "name": "Light 3",
            "hex": "{{lightColor3}}"
          },
          {
            "name": "Dark 1",
            "hex": "{{darkColor1}}"
          },
          {
            "name": "Dark 2",
            "hex": "{{darkColor2}}"
          },
          {
            "name": "Dark 3",
            "hex": "{{darkColor3}}"
          }
        ],
        "typography": {
          "primaryFont": {
            "name": "{{primaryFontName}}",
            "weights": "{{primaryFontWeights}}",
            "source": "{{primaryFontSource}}"
          },
          "secondaryFont": {
            "name": "{{secondaryFontName}}",
            "weights": "{{secondaryFontWeights}}",
            "source": "{{secondaryFontSource}}"
          }
        },
        "backgroundImages": {
          "hero": {
            "url": "{{heroBackgroundImageUrl}}",
            "description": "{{heroBackgroundDescription}}",
            "parallaxEffect": "{{heroParallaxEffect}}"
          },
          "sections": [
            {
              "sectionName": "{{sectionName}}",
              "url": "{{sectionBackgroundImageUrl}}",
              "description": "{{sectionBackgroundDescription}}",
              "parallaxEffect": "{{sectionParallaxEffect}}",
              "contrastEnsured": true
            }
          ]
        }
      },
      "structure": {
        "pages": [
          {
            "name": "{{pageName}}",
            "path": "{{pagePath}}",
            "layout": "{{pageLayoutDescription}}"
          }
        ],
        "forms": {
          "quoteForm": {
            "type": "simple-quote",
            "fields": [
              "{{quoteFormField1}}",
              "{{quoteFormField2}}",
              "{{quoteFormField3}}"
            ],
            "purpose": "For businesses that apply for quotes",
            "submitAction": "{{quoteFormSubmitAction}}"
          },
          "contactForm": {
            "type": "contact-us",
            "fields": [
              "{{contactFormField1}}",
              "{{contactFormField2}}",
              "{{contactFormField3}}"
            ],
            "purpose": "General contact form",
            "submitAction": "{{contactFormSubmitAction}}"
          }
        },
        "layouts": {
          "scrollEffects": "{{scrollEffectsDescription}}",
          "backgroundImageReveals": "{{backgroundImageRevealDescription}}",
          "ctaContrast": "{{ctaContrastDescription}}"
        }
      }
    },
    "PromptHints": "This gives guidance to any AI creating an example or just putting data in this prompt. This is going to help you enhance the final json. You need to use more background images and layouts. We need atleast two types of forms, free simple quotes forms (for businesses that apply), and contact us form. More creative font choices. Also incorporate like a still background images but you can see different parts of it as you scroll."
  }