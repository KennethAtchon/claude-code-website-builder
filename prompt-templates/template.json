{
    "website": {
      "name": "{{websiteName}}",
      "theme": {
        "name": "{{themeName}}",
        "aesthetic": "{{themeAesthetic}}",
        "mood": "{{themeMood}}",
        "targetAudience": "{{themeTargetAudience}}",
        "designPrinciples": "{{themeDesignPrinciples}}",
        "gradientStyle": "{{themeGradientStyle}}"
      },
      "styles": {
        "recommendedColors": [
          "#4361EE",
          "#22C55E",
          "#EF4444",
          "#8B5CF6",
          "#F59E0B",
          "#06B6D4",
          "#EC4899",
          "#10B981",
          "#3B82F6",
          "#F97316",
          "#8B5A2B",
          "#DC2626",
          "#7C3AED",
          "#059669",
          "#0EA5E9",
          "#E11D48",
          "#7C2D12",
          "#1E40AF",
          "#9333EA",
          "#0D9488"
        ],
        "colorPaletteScript": "node ../color-palette-generator.js \"{{primaryColor}}\"",
        "colorPaletteScriptDescription": "USE THE SCRIPT, DONT FILL IN - This script generates a complete 8-color palette from a single primary color. It outputs a JSON array with harmonious variations (Primary, Primary Dark, Light 1-3, Dark 1-3) while maintaining proper contrast ratios for accessibility. Use this when only a primary color is provided to automatically generate the full color scheme.",
        "colors": [
          {
            "name": "Primary",
            "hex": "{{primaryColor}}"
          },
          {
            "name": "Primary Dark",
            "hex": "{{primaryDarkColor}}"
          },
          {
            "name": "Light 1",
            "hex": "{{lightColor1}}"
          },
          {
            "name": "Light 2",
            "hex": "{{lightColor2}}"
          },
          {
            "name": "Light 3",
            "hex": "{{lightColor3}}"
          },
          {
            "name": "Dark 1",
            "hex": "{{darkColor1}}"
          },
          {
            "name": "Dark 2",
            "hex": "{{darkColor2}}"
          },
          {
            "name": "Dark 3",
            "hex": "{{darkColor3}}"
          }
        ],
        "typography": {
          "primaryFont": {
            "name": "{{primaryFontName}}",
            "weights": "{{primaryFontWeights}}",
            "source": "{{primaryFontSource}}"
          },
          "secondaryFont": {
            "name": "{{secondaryFontName}}",
            "weights": "{{secondaryFontWeights}}",
            "source": "{{secondaryFontSource}}"
          }
        },
        "backgroundImages": {
          "hero": {
            "url": "{{heroBackgroundImageUrl}}",
            "description": "{{heroBackgroundDescription}}",
            "parallaxEffect": "{{heroParallaxEffect}}"
          },
          "sections": [
            {
              "sectionName": "{{sectionName}}",
              "url": "{{sectionBackgroundImageUrl}}",
              "description": "{{sectionBackgroundDescription}}",
              "parallaxEffect": "{{sectionParallaxEffect}}",
              "contrastEnsured": true
            }
          ]
        }
      },
      "structure": {
        "pages": [
          {
            "name": "{{pageName}}",
            "path": "{{pagePath}}",
            "layout": "{{pageLayoutDescription}}"
          }
        ],
        "forms": {
          "quoteForm": {
            "type": "simple-quote",
            "fields": [
              "{{quoteFormField1}}",
              "{{quoteFormField2}}",
              "{{quoteFormField3}}"
            ],
            "purpose": "For businesses that apply for quotes",
            "submitAction": "{{quoteFormSubmitAction}}"
          },
          "contactForm": {
            "type": "contact-us",
            "fields": [
              "{{contactFormField1}}",
              "{{contactFormField2}}",
              "{{contactFormField3}}"
            ],
            "purpose": "General contact form",
            "submitAction": "{{contactFormSubmitAction}}"
          }
        },
        "layouts": {
          "scrollEffects": "{{scrollEffectsDescription}}",
          "backgroundImageReveals": "{{backgroundImageRevealDescription}}",
          "ctaContrast": "{{ctaContrastDescription}}"
        }
      }
    },
    "cleanup": "When this script is copied over to an example. Remove text that isn't necessary to give the AI full context to build the website (recommendedColors, colorPaletteScript, etc)."
  }