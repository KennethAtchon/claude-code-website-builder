          },
          {
            "type": "testimonials_preview",
            "style": "{{home_testimonials_style}}"
          },
          {
            "type": "cta",
            "style": "{{home_cta_style}}"
          }
        ]
      },
      {
        "page_name": "about",
        "sections": [
          {
            "type": "hero",
            "style": "{{about_hero_style}}"
          },
          {
            "type": "company_story",
            "style": "{{about_story_style}}"
          },
          {
            "type": "mission_values",
            "style": "{{about_mission_style}}"
          },
          {
            "type": "team",
