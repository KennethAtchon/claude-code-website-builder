            "style": "{{services_testimonials_style}}"
          },
          {
            "type": "cta",
            "style": "{{services_cta_style}}"
          }
        ]
      },
      {
        "page_name": "portfolio",
        "sections": [
          {
            "type": "hero",
            "style": "{{portfolio_hero_style}}"
          },
          {
            "type": "portfolio_gallery",
            "style": "{{portfolio_gallery_style}}"
          },
          {
            "type": "case_studies",
            "style": "{{portfolio_case_studies_style}}"
          },
          {
            "type": "cta",
            "style": "{{portfolio_cta_style}}"
          }
        ]
      },
      {
        "page_name": "testimonials",
        "sections": [
          {
            "type": "hero",
