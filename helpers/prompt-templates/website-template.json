  },

  "features": {
    "blog": "{{include_blog}}",
    "ecommerce": "{{include_shop}}",
    "booking": "{{include_booking}}",
    "multilingual": "{{include_languages}}",
    "search": "{{include_search}}",
    "newsletter": "{{include_newsletter}}",
    "live_chat": "{{include_chat}}",
    "analytics": "{{include_analytics}}"
  },

  "seo": {
    "meta_title": "{{seo_title}}",
