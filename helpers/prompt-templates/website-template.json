    "keywords": ["{{keyword_1}}", "{{keyword_2}}", "{{keyword_3}}"],
    "local_seo": "{{enable_local_seo}}",
    "schema_markup": true
  },

  "content_strategy": {
    "tone": "{{brand_tone}}",
    "target_audience": "{{target_audience}}",
