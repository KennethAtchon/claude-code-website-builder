{
    "website": {
      "name": "{{websiteName}}",
      "type": "{{websiteType}}",
      "description": "{{websiteDescription}}",
      "industry": "{{websiteIndustry}}",
      "theme": {
        "name": "{{themeName}}",
        "aesthetic": "{{themeAesthetic}}",
        "mood": "{{themeMood}}",
        "targetAudience": "{{themeTargetAudience}}",
        "designPrinciples": "{{themeDesignPrinciples}}",
        "gradientStyle": "{{themeGradientStyle}}"
      },
      "styles": {
        "pickRandomlyFromRecommendedColors": [
          "#4361EE",
          "#22C55E",
          "#EF4444",
          "#8B5CF6",
          "#F59E0B",
          "#06B6D4",
          "#EC4899",
          "#10B981",
          "#3B82F6",
          "#F97316",
          "#8B5A2B",
          "#DC2626",
          "#7C3AED",
          "#059669",
          "#0EA5E9",
          "#E11D48",
          "#7C2D12",
          "#1E40AF",
          "#9333EA",
          "#0D9488"
        ],
        "colorPaletteScript": "node ../color-palette-generator.js \"{{primaryColor}}\"",
        "colorPaletteScriptDescription": "USE THE SCRIPT, DONT FILL IN - This script generates a complete 8-color palette from a single primary color. It outputs a JSON array with harmonious variations (Primary, Primary Dark, Light 1-3, Dark 1-3) while maintaining proper contrast ratios for accessibility. Use this when only a primary color is provided to automatically generate the full color scheme.",
        "colors": [
          {
            "name": "Primary",
            "hex": "{{primaryColor}}"
          },
          {
            "name": "Primary Dark",
            "hex": "{{primaryDarkColor}}"
          },
          {
            "name": "Light 1",
            "hex": "{{lightColor1}}"
          },
          {
            "name": "Light 2",
            "hex": "{{lightColor2}}"
          },
          {
            "name": "Light 3",
            "hex": "{{lightColor3}}"
          },
          {
            "name": "Dark 1",
            "hex": "{{darkColor1}}"
          },
          {
            "name": "Dark 2",
            "hex": "{{darkColor2}}"
          },
          {
            "name": "Dark 3",
            "hex": "{{darkColor3}}"
          }
        ],
        "typography": {
          "primaryFont": {
            "name": "{{primaryFontName}}",
            "weights": "{{primaryFontWeights}}",
            "source": "{{primaryFontSource}}"
          },
          "secondaryFont": {
            "name": "{{secondaryFontName}}",
            "weights": "{{secondaryFontWeights}}",
            "source": "{{secondaryFontSource}}"
          }
        }
      },
      "structure": {
        "pages": [
          {
            "name": "{{pageName}}",
            "path": "{{pagePath}}",
            "layout": "{{pageLayoutDescription}}",
            "sections": [
              {
                "name": "{{sectionName}}",
                "type": "{{sectionType}}",
                "content": "{{sectionContentDescription}}",
                "media": {
                  "image": "{{sectionImageUrl}}",
                  "video": "{{sectionVideoUrl}}"
                },
                "cta": {
                  "label": "{{ctaLabel}}",
                  "link": "{{ctaLink}}"
                }
              }
            ]
          }
        ],
        "layouts": {
          "layoutStyle": "{{layoutStyle}}",
          "navigationStyle": "{{navigationStyle}}",
          "extra": "{{extraLayoutRequirements}}"
        }
      }
    },
    "templateInfo": {
      "version": "2.0",
      "type": "generic",
      "description": "Universal website template that can be adapted for any type of website - business, portfolio, blog, e-commerce, nonprofit, educational, etc.",
      "supportedWebsiteTypes": [
        "business",
        "portfolio", 
        "blog",
        "e-commerce",
        "nonprofit",
        "educational",
        "landing-page",
        "corporate",
        "creative",
        "personal",
        "startup",
        "agency"
      ],
      "cleanup": "When this template is used for a specific website, customize the placeholder values and remove unnecessary sections based on the website type and requirements."
    }
  }